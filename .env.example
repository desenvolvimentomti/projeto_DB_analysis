# Environment Configuration
# Copy this file to .env and fill in your actual credentials

# Google Earth Engine Service Account
GEE_SERVICE_ACCOUNT_JSON_PATH=path/to/your/service-account-key.json

# Or use the raw JSON (not recommended for production)
# GEE_SERVICE_ACCOUNT_JSON={"type": "service_account", "project_id": "..."}

# Open-Meteo API (usually no auth needed, but you can add custom parameters)
OPENMETEO_API_URL=https://api.open-meteo.com/v1/forecast

# Data paths
DATA_INPUT_FOLDER=./data
DATA_OUTPUT_FOLDER=./outputs

# Logging
LOG_LEVEL=INFO
